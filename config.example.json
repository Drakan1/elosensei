{
	"general": {
		"description": "Manage Elo ratings and matches",
		"prefix": "elo",
		"space_after_prefix": true, 
		"playing": "MissileWars",
		"token": "your bot token here"
	},
	"elo": {
		"enable": true,
		"log_level": "INFO",
		"match_history_path": "matches.pkl",
		"user_status_path": "players.pkl",
		"default_elo": 1200,
		"default_status_value": 0.0,
		"status_values": {
			"win": 1.0,
			"loss": 0.0,
			"draw": 0.5
		},
		"allow_only_defined_status_values": true,
		"k_factor": 50,
		"max_teams": 2,
		"score_limit": 1.0,
		"require_score_limit": true,
		"ranks": [
			{
				"name": "Bronze",
				"color": "AAAAAA",
				"cutoff": 0,
				"default": true
			},
			{
				"name": "Silver",
				"color": "AAAAAA",
				"cutoff": 1350,
				"default": false
			},
			{
				"name": "Gold",
				"color": "AAAAAA",
				"cutoff": 1500,
				"default": false
			}
		],
		"ranks_use_raw_elo": false,
		"max_top": 16,
		"max_player_cards": 4,
		"max_match_cards": 4,
		"queries_need_admin_perms": false,
		"player_perms_need_role": false,
		"player_role_name": "Player",
		"admin_role_name": "Elo Manager",
		"periodic_save": true,
		"periodic_save_interval": 1000,
		"save_on_shutdown": true
	},
	"eggs": {
		"enable": true,
		"default_egg": "plot",
		"eggs": {
			"plot": "eggs/plot.png"
		}
	}
}

